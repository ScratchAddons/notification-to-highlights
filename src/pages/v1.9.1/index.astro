---
import DisplayIndex from '../../layouts/DisplayIndex.astro'
const locales: any = {};

await Object.entries(
	import.meta.glob("./_locales/*/messages.json", { query: "?raw", import: "default" })
).forEach(async ([k, v]) => {
	const languageCode = k.split("/")[2];
	locales[languageCode] = v();
});

const { lang } = Astro.params;
---

<DisplayIndex version="v1.9.1" locales={ locales } lang={ lang }>

</DisplayIndex>

